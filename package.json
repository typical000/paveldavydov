{
  "name": "paveldavydov",
  "private": true,
  "version": "0.1.0",
  "description": "Personal site",
  "author": "Pavel Davydov",
  "engines": {
    "node": "0.6.x"
  },
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development PORT=3000 node ./bin/webpack-dev-server.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod",
    "build:analyze": "cross-env NODE_ENV=production webpack --config ./webpack.config.analyze",
    "build:html": "cross-env NODE_ENV=production node ./bin/build-html",
    "clean": "rimraf 'dist/*'",
    "all:dev": "npm run clean && npm run build:dev && npm run build:html",
    "all:prod": "npm run clean && npm run build:prod && npm run build:html",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "start": "npm run lint && npm run all:prod"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "common-tags": "^1.4.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-minifier": "^3.2.3",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.2.5",
    "module-alias": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.2.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "classnames": "^2.2.5",
    "css-functions": "^1.0.5",
    "gsap": "^1.19.1",
    "jss": "^6.3.0",
    "jss-preset-default": "^1.2.0",
    "normalize-jss": "^3.0.0",
    "pixi.js": "^4.3.5",
    "preact": "^7.2.0",
    "preact-compat": "^3.14.1",
    "react-jss": "^5.1.1",
    "whatwg-fetch": "^2.0.1"
  },
  "lint-staged": {
    "./src ./tests ./*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "license": "MIT"
}
